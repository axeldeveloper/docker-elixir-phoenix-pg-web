<h1>Show Order</h1>

<ul>

  <li>
    <strong>Ordered:</strong>
    <%= @order.ordered %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @order.description %>
  </li>

  <li>
    <strong>Value:</strong>
    <%= @order.value %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.order_path(@conn, :edit, @order) %></span>
<span><%= link "Back", to: Routes.order_path(@conn, :index) %></span>
