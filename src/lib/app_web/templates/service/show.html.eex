<h1>Show Service</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @service.name %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @service.description %>
  </li>

  <li>
    <strong>Value:</strong>
    <%= @service.value %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.service_path(@conn, :edit, @service) %></span>
<span><%= link "Back", to: Routes.service_path(@conn, :index) %></span>
